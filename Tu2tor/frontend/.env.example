# Google Gemini API Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_MODEL=gemini-pro

# Ollama Configuration (Local AI)
VITE_OLLAMA_BASE_URL=http://localhost:11434
VITE_OLLAMA_MODEL=llama2

# GPT-OSS Alternatives
VITE_HUGGINGFACE_API_KEY=your_huggingface_key_here
VITE_TOGETHER_API_KEY=your_together_key_here

# AI Feature Toggles
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_AI_RECOMMENDATIONS=true
VITE_ENABLE_CONTENT_GENERATION=true
VITE_ENABLE_SEMANTIC_SEARCH=true

# Usage Limits & Cost Control
VITE_MAX_TOKENS_PER_REQUEST=2000
VITE_DAILY_TOKEN_LIMIT=100000
VITE_DEFAULT_AI_PROVIDER=gemini

# Development Mode
VITE_AI_DEBUG_MODE=false
